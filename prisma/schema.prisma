generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id   Int    @id @default(autoincrement())
  url  String
  tags Tag[]

  @@map("link")
}

model Tag {
  id    Int    @id @default(autoincrement())
  name  String @unique
  links Link[]

  @@map("tag")
}
